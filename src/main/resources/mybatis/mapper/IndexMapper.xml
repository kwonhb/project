<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.dao.IndexDao">
<select id="boardbest" resultType="com.spring.dto.BoardDto">
	select * from (select * from board order by readcount desc) where rownum &gt;= 1 and rownum &lt;= 5
</select>
</mapper>